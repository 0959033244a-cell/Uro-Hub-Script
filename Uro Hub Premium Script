local C = game:GetService("CoreGui")
local H = game:GetService("HttpService")
local U = "WACK"

local F = {
    ["üèÉ ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà"] = {
